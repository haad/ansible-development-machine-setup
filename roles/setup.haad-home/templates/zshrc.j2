ZSH=$HOME/.oh-my-zsh
ZSH_THEME="haad"

ZSH_DISABLE_COMPFIX="true"
DISABLE_AUTO_TITLE="false"

export GOPATH="${HOME}/Devel/go"
export PATH=/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:${GOPATH}/bin:$PATH

export HOMEBREW_GITHUB_API_TOKEN="{{ userconf_homebrew_github_api_token }}"
export EDITOR=nvim
export HOSTNAME=$(hostname)

export SHELL=/usr/bin/zsh
export LANG=en_US.UTF-8

plugins=(git gh terraform dotenv jsontools helm ripgrep haad cfcdn cfvault kcfg aws-ssm kubectl aws-vault)

source ~/.oh-my-zsh/oh-my-zsh.sh

